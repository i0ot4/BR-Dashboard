@model City

<ol class="breadcrumb">
    <li class="breadcrumb-item">
        <a asp-controller="Home" asp-action="Index">لوحة التحكم</a>
    </li>
    <li class="breadcrumb-item active">  Examples  </li>
</ol>
<div class="card">

    <div class="card-body">
        @if (TempData.ContainsKey("msg"))
        {
            <div class="alert alert-success"> @TempData["msg"]</div>
        }

        @if (TempData.ContainsKey("error"))
        {
            <div class="alert alert-danger">@TempData["error"]</div>
        }
        <div asp-validation-summary="All" class="alert-danger"></div>
        <h4 class="mb-3">تعديل </h4>
        <form class="row mb-3" asp-action="Edit" method="post">
            <input asp-for="Id" type="hidden">
            
            <input asp-for="IsActive" type="hidden">
            <input asp-for="IsDeleted" type="hidden">

            <div class="form-group col-md-6 col-sm-12">
                <label class="form-label" asp-for="CityName"> </label><br />

                <input asp-for="CityName" class="form-control" placeholder="">
                <span class="text-danger" asp-validation-for="CityName"></span>
                <small class="text-muted"></small>
            </div>
            <div class="form-group col-md-12 col-sm-12 mt-4 ">
                <a class="btn btn-secondary  w-20" asp-action="Index">الغاء</a>
                <input type="submit" class="btn btn-primary w-40 " value="حفظ">
            </div>
        </form>
    </div>
</div>